# Creating a set using curly braces
myset = {"apple", "banana", "cherry"}
print(type(myset))

# Creating a set using double round-brackets
thisset = set(("apple", "banana", "cherry"))
print(thisset)

# Iterating through the set and printing each element
for x in thisset:
    print(x)

# Adding "orange" to the set
thisset.add("orange")
print(thisset)

# Creating sets and updating them
thisset = {"apple", "banana", "cherry"}
tropical = {"pineapple", "mango", "papaya"}
thisset.update(tropical)
print(thisset)

# Updating set with a list
thisset = {"apple", "banana", "cherry"}
mylist = ["kiwi", "orange"]
thisset.update(mylist)
print(thisset)

# Removing elements from the set
thisset.remove("banana")
thisset.discard("orange")
thisset.pop()
thisset.clear()
print(thisset)

# Performing set operations
set1 = {"USA", "UK", "India"}
set2 = {1, 2, 3}
set3 = set1.union(set2)
print(set3)

# Updating set using intersection
x = {"apple", "banana", "cherry"}
y = {"google", "microsoft", "apple"}
x.intersection_update(y)
print(x)

# Updating set using symmetric difference
x = {"apple", "banana", "cherry"}
y = {"google", "microsoft", "apple"}
x.symmetric_difference_update(y)
print(x)

# Checking if "apple" is in the set
fruits = {"apple", "banana", "cherry"}
if "apple" in fruits:
    print("Yes, apple is a fruit!")
