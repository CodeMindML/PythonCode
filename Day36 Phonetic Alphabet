
import pandas
data = pandas.read_csv("nato_phonetic_alphabet.csv")
#print(data.to_dict())
phonic_dict = {row.letter: row.code for (index, row) in data.iterrows()}
print(phonic_dict)

word = input("Enter a word : ").upper()
for letter in word:
    output_list = {phonic_dict[letter] for letter in word}
    print(output_list)


